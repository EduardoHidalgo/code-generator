{
  "name": "code-generator",
  "version": "0.1.0",
  "main": "src/server/index.ts",
  "scripts": {
    "installation": "npm install && npm run db:generate",
    "dev": "nodemon --inspect ./src/server/index.ts",
    "start": "node build/src/server/index.js",
    "build": "npm run del-build && npm run db:generate && npm run ts-build && npm run post-build",
    "format": "npx prettier --write 'src/app/**/*.ts'",
    "cli": "npx ts-node -r tsconfig-paths/register cli/index.ts",
    "del-build": "npx rimraf build",
    "ts-build": "npx tsc -p tsconfig.json",
    "post-build": "npx tsc-alias -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bob-Do-The-Job/violet.git"
  },
  "bugs": {
    "url": "https://github.com/Bob-Do-The-Job/violet/issues"
  },
  "homepage": "https://github.com/Bob-Do-The-Job/violet#readme",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.4.1",
    "helmet": "^4.6.0",
    "inquirer": "^8.2.0",
    "joi": "^17.4.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/inquirer": "^8.1.3",
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.3",
    "@types/node-fetch": "^3.0.3",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.4.2",
    "tsconfig-paths": "^3.11.0"
  }
}
